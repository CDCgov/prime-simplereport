{
  "transform": {
    "\\.[jt]sx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "node_modules\\/(?!(uuid|uswds))"
  ],
  "moduleNameMapper": {
    "\\.(css|less|sass|scss|png|mp3)$": "identity-obj-proxy"
  },
  "setupFilesAfterEnv": ["<rootDir>/jest-setup.ts"],
  "testEnvironment": "jsdom"
}