    "sass": "^1.56.0",
    "stylelint": "^14.15.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "^2.2.2"
  },
  "msw": {
    "workerDirectory": "/storybook_public"
  },
  "resolutions": {
    "trim": "0.0.3 ",
    "trim-newlines": "3.0.1",
    "glob-parent": "5.1.2"
  }
}
