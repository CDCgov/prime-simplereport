mutation addDeviceNew(
    $name: String!
    $manufacturer: String!
    $model: String!
    $loincCode: String!
    $swabTypes: [ID!]!
) {
    createDeviceTypeNew(
      name: $name
      manufacturer: $manufacturer
      model: $model
      loincCode: $loincCode
      swabTypes: $swabTypes
    ) {
      internalId
      name
      model
      manufacturer
      loincCode
      swabTypes
    }
}